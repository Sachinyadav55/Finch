	Journal = {J Virol},
	Journal-Full = {Journal of virology},
	Mesh = {Archaeal Viruses; Base Sequence; DNA, Viral; Fuselloviridae; Genome, Viral; Molecular Sequence Data; Open Reading Frames; Sulfolobus; Viral Proteins; Virus Replication},
